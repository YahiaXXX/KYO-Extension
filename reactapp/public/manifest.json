{
  "manifest_version": 3,
  "name": "chrome extension",
  "description": "react chrome extension",
  "version":"1.0.0",
  "icons": {
    "128": "kyo.png"
 },
  
  "action":{
    "default_popup":"popup.html",
    "default_title":"Open"
  },
  "permissions":[
    "tabs",
    "scripting",
    "storage",
    "activeTab"
    
  ],
  "host_permissions":[
    "http://*/",
    "https://*/",
    "http://localhost:5000/*",
    "https://mail.google.com/*"
  ]
  
}
